       HTR     LOAD              OCTAL LOADER ABSENT                    TMLU0068
       ORG     LOAD8+1                                                  TMLU0069
       ABS                       RESUME STANDARD PUNCHING               TMLU0070
 LOAD9 AXT     2,2               SET TO COUNT FIELD PAIRS               TMLU0071
       CAL     9L+18,4           ROW                                    TMLU0072
       ORS     9L+14,1           ROW UNION                              TMLU0073
       LDQ     9L+14,1                                                  TMLU0074
       TXI     LOD11,2,22        SET TO PEEL OFF SIX BITS               TMLU0075
 LOD10 TXH     *+2,4,2           SKIP STORE TILL AFTER ONE-ROW          TMLU0076
       SLW     ****              STORE OCTAL CORRECTION                 TMLU0077
       TIX     *-3,2,1           ADVANCE TO NEXT PAIR, THIS HALF        TMLU0078
       TIX     LOAD9,4,2         ADVANCE TO NEXT ROW                    TMLU0079
       TNX     LOAD+1,5,13       OUT AFTER RIGHT HALF                   TMLU0080
       RCDA                      START NEXT CARD                        TMLU0081
       TXI     LOAD9,5,12        ADVANCE TO RIGHT HALF CARD             TMLU0082
LOD11  CLM                       CLEAR AC                               TMLU0083
       ALS     2                                                        TMLU0084
       LGL     1                 PEEL OFF BITS                          TMLU0085
       TIX     LOD11+1,2,4       COUNT COLUMNS PER FIELD                TMLU0086
       TXH     *+2,4,12          USE 7-ROW AS FIRST SUM                 TMLU0087
       ACL     11L+4,2           ADD PREVIOUS SUM                       TMLU0088
       SLW     11L+4,2           NEW PARTIAL SUM                        TMLU0089
       TNX     LOD10,2,2         OUT IF SECOND FIELD OF PAIR            TMLU0090
       STA     LOD10+1           STORE ADDRESS OF CORRECTION            TMLU0091
       TXI     LOD11,2,44        RETURN TO PEEL OFF 12 BITS             TMLU0092
       ORG     LOAD-24                                                  TMLU0093
 9L    BSS     24                INPUT BUFFER                           TMLU0094
 9R    SYN     9L+1              CARD CHECK SUM                         TMLU0095
 11L   SYN     9L+20             TEMPORARY FOR OCTAL                    TMLU0096
*              INSERT AS50X000 HERE                                     TMLU0097
