      SUBROUTINE VCTR   (A,V,N,ALPHA)                                   VCTR0001
      DIMENSION A(50,50),V(50)                                          VCTR0002
      A(1,1)=A(1,1)-ALPHA                                               VCTR0003
 6    DO 15 I=2,N                                                       VCTR0004
      A(I,I)=A(I,I)-ALPHA                                               VCTR0005
 70   II=I-1                                                            VCTR0006
 7    DO 15 J=1,II                                                      VCTR0007
 8    IF (A(I,J))9,15,9                                                 VCTR0008
 9    IF (ABSF(A(J,J))-ABSF(A(I,J)))11,10,10                            VCTR0009
 10   R=A(I,J)/A(J,J)                                                   VCTR0010
      GO TO 130                                                         VCTR0011
 11   R=A(J,J)/A(I,J)                                                   VCTR0012
      DO 12 K=1,N                                                       VCTR0013
      C=A(J,K)                                                          VCTR0014
      A(J,K)=A(I,K)                                                     VCTR0015
 12   A(I,K)=C                                                          VCTR0016
 130  JJ=J+1                                                            VCTR0017
 13   DO 14 K=JJ,N                                                      VCTR0018
 14   A(I,K)=A(I,K)-R*A(J,K)                                            VCTR0019
 15   CONTINUE                                                          VCTR0020
      C=A(N,N)                                                          VCTR0021
      V(N)=1.                                                           VCTR0022
      DO 29 I=2,N                                                       VCTR0023
      JJ=N-I+1                                                          VCTR0024
      R=0.                                                              VCTR0025
      II=N-I+2                                                          VCTR0026
      DO 25 K=II,N                                                      VCTR0027
 25   R=R+A(JJ,K)*V(K)                                                  VCTR0028
      IF (ABSF(A(JJ,JJ))-1.0E-10)27,27,28                               VCTR0029
 27   V(JJ)=1.                                                          VCTR0030
      C=0.                                                              VCTR0031
      DO 26 J=II,N                                                      VCTR0032
 26   V(J)=0.                                                           VCTR0033
      GO TO 29                                                          VCTR0034
 28   V(JJ)=(C-R)/A(JJ,JJ)                                              VCTR0035
 29   CONTINUE                                                          VCTR0036
      RETURN                                                            VCTR0037
      END (2,2,2,2,2)                                                   VCTR0038
