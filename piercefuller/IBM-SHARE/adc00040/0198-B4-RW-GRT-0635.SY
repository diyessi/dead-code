      SUBROUTINE GRT  (N,C,IN)                                          GRT 0001
      DIMENSION C(50)                                                   GRT 0002
      DO 100 L=1,N                                                      GRT 0003
      JK=0                                                              GRT 0004
      IF (C(L))45,46,45                                                 GRT 0005
 45   RT=.9*C(L)                                                        GRT 0006
      ASSIGN 1 TO NN                                                    GRT 0007
      GO TO 80                                                          GRT 0008
 1    X0=FPRT                                                           GRT 0009
      RT=1.1*C(L)                                                       GRT 0010
      ASSIGN 2 TO NN                                                    GRT 0011
      GO TO 80                                                          GRT 0012
 2    X1=FPRT                                                           GRT 0013
      RT=C(L)                                                           GRT 0014
      ASSIGN 3 TO NN                                                    GRT 0015
      GO TO 80                                                          GRT 0016
 3    X2=FPRT                                                           GRT 0017
      GO TO 50                                                          GRT 0018
 46   RT=-1.                                                            GRT 0019
      ASSIGN 4 TO NN                                                    GRT 0020
      GO TO 80                                                          GRT 0021
 4    X0=FPRT                                                           GRT 0022
      RT=1.                                                             GRT 0023
      ASSIGN 5 TO NN                                                    GRT 0024
      GO TO 80                                                          GRT 0025
 5    X1=FPRT                                                           GRT 0026
      RT=0.                                                             GRT 0027
      ASSIGN 6 TO NN                                                    GRT 0028
      GO TO 80                                                          GRT 0029
 6    X2=FPRT                                                           GRT 0030
 50   H=-1.                                                             GRT 0031
      D=-.5                                                             GRT 0032
 49   DD=1.+D                                                           GRT 0033
      BI=(X0*D*D)-(X1*DD*DD)+(X2*(DD+D))                                GRT 0034
      DEN=BI*BI-(4.*X2*D*DD)(X0*D-(X1*DD)+X2)                           GRT 0035
      IF (DEN)52,52,51                                                  GRT 0036
 52   IF (IN)36,35,36                                                   GRT 0037
 35   WRITE OUTPUT TAPE 6,25,L,DEN                                      GRT 0038
 36   DEN=0.                                                            GRT 0039
 51   DEN=SQRTF(DEN)                                                    GRT 0040
 53   DN=BI+DEN                                                         GRT 0041
      DM=BI-DEN                                                         GRT 0042
      IF (ABSF(DN)-ABSF(DM))57,57,56                                    GRT 0043
 56   DEN=DN                                                            GRT 0044
      GO TO 58                                                          GRT 0045
 57   DEN=DM                                                            GRT 0046
 58   IF (DEN)55,54,55                                                  GRT 0047
 54   DEN=1.                                                            GRT 0048
 55   DI=(-2.*X2*DD)/DEN                                                GRT 0049
      H=DI*H                                                            GRT 0050
      RT=RT+H                                                           GRT 0051
      IF (ABSF(H/RT)-1.0E-6)75,75,60                                    GRT 0052
 60   ASSIGN 7 TO NN                                                    GRT 0053
      GO TO 80                                                          GRT 0054
 7    IF (ABSF(FPRT)-ABSF(X2*10.))62,61,61                              GRT 0055
 61   DI=DI*.5                                                          GRT 0056
      H=H*.5                                                            GRT 0057
      RT=RT-H                                                           GRT 0058
      GO TO 80                                                          GRT 0059
 62   X0=X1                                                             GRT 0060
      X1=X2                                                             GRT 0061
      X2=FPRT                                                           GRT 0062
      D=DI                                                              GRT 0063
      GO TO 49                                                          GRT 0064
 75   CALL AUX (RT,FRT)                                                 GRT 0065
 76   C(L)=RT                                                           GRT 0066
      IF (IN)100,77,100                                                 GRT 0067
 77   WRITE OUTPUT TAPE 6,26,L,RT,FRT                                   GRT 0068
 26   FORMAT (I3,2E20.8/1H )                                            GRT 0069
 100  CONTINUE                                                          GRT 0070
 27   FORMAT (//)                                                       GRT 0071
      IF (IN)33,34,33                                                   GRT 0072
 34   WRITE OUTPUT TAPE 6,28                                            GRT 0073
 33   RETURN                                                            GRT 0074
 80   JK=JK+1                                                           GRT 0075
      IF (100-JK)75,75,86                                               GRT 0076
 86   CALL AUX (RT,FRT)                                                 GRT 0077
      FPRT=FRT                                                          GRT 0078
      IF (L-1)81,91,81                                                  GRT 0079
 81   DO 82 I=2,L                                                       GRT 0080
      TEM=RT-C(I-1)                                                     GRT 0081
      IF (ABSF(TEM)-1.0E-20)85,82,82                                    GRT 0082
 82   FPRT=FPRT/TEM                                                     GRT 0083
 91   IF (IN) 89,83,89                                                  GRT 0084
 83   WRITE OUTPUT TAPE 6,25,L,RT,FRT,FPRT                              GRT 0085
 89   IF (ABSF(FRT)-1.0E-20)90,84,84                                    GRT 0086
 90   IF (ABSF (FPRT)-1.0E-20)76,84,84                                  GRT 0087
 84   GO TO NN,(1,2,3,4,5,6,7)                                          GRT 0088
 85   RT=RT+.001                                                        GRT 0089
      IF (IN) 88,87,88                                                  GRT 0090
 87   WRITE OUTPUT TAPE 6,25,L                                          GRT 0091
 88   GO TO 80                                                          GRT 0092
 28   FORMAT (1H1)                                                      GRT 0093
 25   FORMAT (I3,3E20.8)                                                GRT 0094
      END (2,2,2,2,2)                                                   GRT 0095
