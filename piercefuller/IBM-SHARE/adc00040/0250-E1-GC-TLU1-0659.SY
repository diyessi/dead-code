C     GCTLU1---TABLE READ IN AND TABLE LOOKUP INTERPOLATION ROUTINE FOR         
C              FUNCTIONS OF ONE, TWO, OR THREE VARIABLES.                       
C                                                                               
C     FOLLOWING NAMED VARIABLES ARE USED....NUMTBL,MANDAN,NUMPTS,IATLU1,        
C     IBTLU1,ICTLU1,IDTLU1,IETLU1,IFTLU1,IGTLU1,IHTLU1,IITLU1,IJTLU1,L1,        
C     L2,IKTLU1,ILTLU1,IMTLU1,INTLU1,IOTLU1,IPTLU1,IQTLU1,IRTLU1,ISTLU1,        
C     DUMSTG,STG,NG,NORMAL,MINTBL,MAXTBL,ARG1,ARG2,ARG3,FCT                     
C                                                                               
C     STATEMENT NUMBERS USED ARE 55 THRU 113                                    
C                                                                               
      DIMENSION L1(5),L2(5),STG(300),NUMPTS(5),DUMSTG(10)                       
   55 NUMTBL=1                                                          TLU1 001
      MANDAN=0                                                          TLU1 002
      NUMPTS(1)=0                                                       TLU1 003
   56 IF (IATLU1) 110,103,102                                           TLU1 004
  102 READ INPUT TAPE IATLU1,57,IBTLU1,ICTLU1,IDTLU1,IETLU1             TLU1 005
   57 FORMAT(8H        I4,2I2,I56)                                      TLU1 006
  104 IF (IETLU1) 111,58,111                                            TLU1 007
   58 IF (IBTLU1) 99,99,59                                              TLU1 008
   59 L1(NUMTBL)=ICTLU1                                                 TLU1 009
      L2(NUMTBL)=IDTLU1                                                 TLU1 010
      IGTLU1=(L1(NUMTBL)-1)/9+1                                         TLU1 011
      DO 68 IHTLU1=1,IGTLU1                                             TLU1 012
      IITLU1=9*IHTLU1-8                                                 TLU1 013
      IF (IHTLU1-IGTLU1) 60,61,60                                       TLU1 014
   60 IJTLU1=IITLU1+8                                                   TLU1 015
      GO TO 62                                                          TLU1 016
   61 IJTLU1=L1(NUMTBL)                                                 TLU1 017
   62 IKTLU1=NUMPTS(NUMTBL)+1                                           TLU1 018
      ILTLU1=IKTLU1+IITLU1                                              TLU1 019
      IMTLU1=IKTLU1+IJTLU1                                              TLU1 020
      INTLU1=0                                                          TLU1 021
      IPTLU1=IKTLU1+L1(NUMTBL)                                          TLU1 022
      IQTLU1=IPTLU1+L2(NUMTBL)                                          TLU1 023
   63 IF (IATLU1) 110,106,105                                           TLU1 024
  105 READ INPUT TAPE IATLU1,64,(DUMSTG(IBTLU1),IBTLU1=1,10),IETLU1     TLU1 025
   64 FORMAT(10E7.0,I2)                                                 TLU1 026
  107 STG(IKTLU1)=DUMSTG(1)                                             TLU1 027
      IBTLU1=2                                                          TLU1 028
      DO 65 IRTLU1=ILTLU1,IMTLU1                                        TLU1 029
      STG(IRTLU1)=DUMSTG(IBTLU1)                                        TLU1 030
   65 IBTLU1=IBTLU1+1                                                   TLU1 031
      IF(IETLU1-(IHTLU1*L2(NUMTBL)-L2(NUMTBL)+IHTLU1+INTLU1))111,66,111 TLU1 032
   66 ILTLU1=IQTLU1+IITLU1                                              TLU1 033
      IMTLU1=IQTLU1+IJTLU1                                              TLU1 034
      IKTLU1=IPTLU1+1+INTLU1                                            TLU1 035
      IF(INTLU1-L2(NUMTBL))67,68,110                                    TLU1 036
   67 INTLU1=INTLU1+1                                                   TLU1 037
      IQTLU1=IQTLU1+L1(NUMTBL)                                          TLU1 038
      GO TO 63                                                          TLU1 039
   68 CONTINUE                                                          TLU1 040
      IF (MANDAN) 108,109,108                                           TLU1 041
  109 NUMPTS(NUMTBL+1)=NUMPTS(NUMTBL)+L1(NUMTBL)*L2(NUMTBL)+L1(NUMTBL)+1TLU1 042
     1+L2(NUMTBL)                                                       TLU1 043
  108 NUMTBL=NUMTBL+1                                                   TLU1 044
      GO TO 56                                                          TLU1 045
   69 GO TO (509,511,513,515),NG                                        TLU1 046
   70 IF (MINTBL-MAXTBL) 71,100,110                                     TLU1 047
   71 DO 73 IITLU1=MINTBL,MAXTBL                                        TLU1 048
      IJTLU1=NUMPTS(IITLU1)+1                                           TLU1 049
      IF (ARG3-STG(IJTLU1)) 72,74,73                                    TLU1 050
   72 IF (IITLU1-MINTBL) 110,112,75                                     TLU1 051
   73 CONTINUE                                                          TLU1 052
      GO TO 112                                                         TLU1 053
   75 IKTLU1=1                                                          TLU1 054
      ILTLU1=2                                                          TLU1 055
      IMTLU1=IJTLU1                                                     TLU1 056
  101 DO 97 IFTLU1=IKTLU1,ILTLU1                                        TLU1 057
      IJTLU1=NUMPTS(IITLU1)+1                                           TLU1 058
      IOTLU1=L1(IITLU1)                                                 TLU1 059
      IPTLU1=IOTLU1+IJTLU1                                              TLU1 060
      DO 77 IQTLU1=1,IOTLU1                                             TLU1 061
      INTLU1=IJTLU1+IQTLU1                                              TLU1 062
      IF (ARG1-STG(INTLU1)) 76,78,77                                    TLU1 063
   76 IF (IQTLU1-1) 110,112,79                                          TLU1 064
   77 CONTINUE                                                          TLU1 065
      GO TO 112                                                         TLU1 066
   78 IGTLU1=-1                                                         TLU1 067
      GO TO 80                                                          TLU1 068
   79 IGTLU1=+1                                                         TLU1 069
   80 IBTLU1=L2(IITLU1)                                                 TLU1 070
      DO 82 IATLU1=1,IBTLU1                                             TLU1 071
      ISTLU1=IPTLU1+IATLU1                                              TLU1 072
      IF (ARG2-STG(ISTLU1)) 81,83,82                                    TLU1 073
   81 IF (IATLU1-1) 110,112,84                                          TLU1 074
   82 CONTINUE                                                          TLU1 075
      GO TO 112                                                         TLU1 076
   83 IHTLU1=-1                                                         TLU1 077
      GO TO 85                                                          TLU1 078
   84 IHTLU1=+1                                                         TLU1 079
   85 IETLU1=IPTLU1+L2(IITLU1)+IQTLU1+IOTLU1*IATLU1-IOTLU1              TLU1 080
      IRTLU1=IETLU1-IOTLU1                                              TLU1 081
      IF (IGTLU1+IHTLU1) 86,88,91                                       TLU1 082
   86 IF (STG(IETLU1)-99999.E9) 87,113,113                              TLU1 083
   87 FCT=STG(IETLU1)                                                   TLU1 084
      GO TO 95                                                          TLU1 085
   88 IF (IGTLU1) 89,110,93                                             TLU1 086
   89 IF (MAX1F(STG(IETLU1),STG(IRTLU1))-99999.E9) 90,113,113           TLU1 087
   90 FCT=STG(IETLU1)-(STG(ISTLU1)-ARG2)*(STG(IETLU1)-STG(IRTLU1))/(STG(TLU1 088
     1ISTLU1)-STG(ISTLU1-1))                                            TLU1 089
      GO TO 95                                                          TLU1 090
   91 IF (MAX1F(STG(IETLU1),STG(IRTLU1),STG(IETLU1-1),STG(IRTLU1-1))-999TLU1 091
     199.E9) 92,113,113                                                 TLU1 092
   92 FCT=((STG(ISTLU1)-ARG2)*((STG(INTLU1)-ARG1)*STG(IRTLU1-1)-(STG(INTTLU1 093
     1LU1-1)-ARG1)*STG(IRTLU1))-(STG(ISTLU1-1)-ARG2)*((STG(INTLU1)-ARG1)TLU1 094
     2*STG(IETLU1-1)-(STG(INTLU1-1)-ARG1)*STG(IETLU1)))/((STG(ISTLU1)-STTLU1 095
     3G(ISTLU1-1))*(STG(INTLU1)-STG(INTLU1-1)))                         TLU1 096
      GO TO 95                                                          TLU1 097
   93 IF (MAX1F(STG(IETLU1),STG(IETLU1-1))-99999.E9) 94,113,113         TLU1 098
   94 FCT=STG(IETLU1)-(STG(INTLU1)-ARG1)*(STG(IETLU1)-STG(IETLU1-1))/(STTLU1 099
     1G(INTLU1)-STG(INTLU1-1))                                          TLU1 100
   95 GO TO (96,98,99),IFTLU1                                           TLU1 101
   96 DUMSTG=FCT                                                        TLU1 102
   97 IITLU1=IITLU1-1                                                   TLU1 103
   98 FCT=DUMSTG-(STG(IMTLU1)-ARG3)*(DUMSTG-FCT)/(STG(IMTLU1)-STG(IJTLU1TLU1 104
     1))                                                                TLU1 105
   99 GO TO (501,504,505,506),NORMAL                                    TLU1 106
  100 IITLU1=MINTBL                                                     TLU1 107
   74 IKTLU1=3                                                          TLU1 108
      ILTLU1=3                                                          TLU1 109
      GO TO 101                                                         TLU1 110
  103 READ 57,IBTLU1,ICTLU1,IDTLU1,IETLU1                               TLU1 111
      GO TO 104                                                         TLU1 112
  106 READ 64,(DUMSTG(IBTLU1),IBTLU1=1,10),IETLU1                       TLU1 113
      GO TO 107                                                         TLU1 114
  110 NG=2                                                              TLU1 115
      GO TO 69                                                          TLU1 116
  111 NG=1                                                              TLU1 117
      GO TO 69                                                          TLU1 118
  112 NG=3                                                              TLU1 119
      GO TO 69                                                          TLU1 120
  113 NG=4                                                              TLU1 121
      GO TO 69                                                          TLU1 122
