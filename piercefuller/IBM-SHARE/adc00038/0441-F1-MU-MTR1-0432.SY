       REM SQUARE MATRIX TRANSPOSE                                        MUMTR1
 MTR1  SXD COMMON,1        SAVE X1 AND X2                               MTR10001
       SXD COMMON+1,2                                                   MTR10002
       CAL 1,4                                                          MTR10003
       SLW COMMON+2        STORE N AND B                                MTR10004
       ARS 18                                                           MTR10005
       STO COMMON+3        N                                            MTR10006
       LXD COMMON+2,1      LOAD X2 WITH N                               MTR10007
       TXI MTR1+8,1,-1     N-1                                          MTR10008
       TXL MTR1+30,1,0     ZERO MEANS DONE                              MTR10009
       SXD COMMON+2,1      SAVE N-1                                     MTR10010
       LXD MTR1+29,2       4 TO X2                                      MTR10011
       CLA COMMON+2        SET INITIAL ADDRESSES                        MTR10012
       STA MTR1+24,2                                                    MTR10013
       TIX MTR1+12,2,1                                                  MTR10014
       TXI MTR1+15,2,-1    SET X2=0                                     MTR10015
       CLA MTR1+21         FIX JI ADDRESSES                             MTR10016
       ACL COMMON+3                                                     MTR10017
       STA MTR1+21                                                      MTR10018
       STA MTR1+22                                                      MTR10019
       TXI MTR1+20,2,-1    DECREASE X2                                  MTR10020
       CLA **,2            A SUB IJ                                     MTR10021
       LDQ **              A SUB JI                                     MTR10022
       STO **              INTERCHANGE                                  MTR10023
       STQ **,2                                                         MTR10024
       TIX MTR1+15,1,1     DECREASE X1                                  MTR10025
       CLA COMMON+2        PREPARE FOR NEXT ROW                         MTR10026
       ACL COMMON+3        AND COLUMN                                   MTR10027
       ACL MTR1+2                                                       MTR10028
       STA COMMON+2                                                     MTR10029
       TXI MTR1+6,0,4                                                   MTR10030
       LXD COMMON,1        RESTORE X1                                   MTR10031
       LXD COMMON+1,2      RESTORE X2                                   MTR10032
       TRA 2,4                                                          MTR10033
