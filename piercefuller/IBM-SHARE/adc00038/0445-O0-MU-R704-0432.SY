       REM R704  MURA REFLECTED  704                                      MUR704
       ORG 24                                                           R7040001
 MAIN  WPR                 JUSTIFY PAPER                                R7040002
       SPR 1               X                                            R7040003
 A     SLF                 TURN OFF SENSE LITES                         R7040004
       LXD TWFR,1          SET IR1=0                                    R7040005
       CLA P               SET INITIAL ADDRESSES                        R7040006
       STA C               IN COPY LOOP                                 R7040007
       STA K               AND PRINTING LOOP                            R7040008
 B     CAL C               ADVANCE ADDRESSES                            R7040009
       ADD TWFV            IN COPY LOOP BY 25                           R7040010
       STA C               X                                            R7040011
       STA E               X                                            R7040012
       LXA TWFR,2          SET IR2=24                                   R7040013
       RCD                 SELECT READER                                R7040014
       PXD 0,0             CLEAR AC                                     R7040015
 C     CAD 0,2         READ COPY LOOP                                   R7040016
       TRA D               TO LOGICAL SUM                               R7040017
       TRA G               END OF FILE, TO PRINT                        R7040018
       HPR 0               END OF CARD, STOP, ERROR                     R7040019
 D     TIX C,2,1           REPEAT UNTIL CARD COPIED                     R7040020
       TZE G               IF SUM =O, TO PRINT                          R7040021
 E     SLW 0               IF SUM NOT O, STORE IT                       R7040022
       TXI F,1,1           INCREASE IR1 BY 1                            R7040023
 F     TXL B,1,299         IF IR1 LESS THAN 300, NEXT CARD              R7040024
       SLN 1               IF IR1=300, TURN ON LITE 1                   R7040025
 G     TXH J,1,0           IF IR1 MORE THAN O, PRINT                    R7040026
       SLT 1               IF IR1=0, TEST LITE 1                        R7040027
       TRA H               OFF, END OF DECK                             R7040028
       TRA A               ON, MORE DECK                                R7040029
 H     HPR 32767           STOP, SHOWING 77777                          R7040030
       TRA MAIN            BEGIN AGAIN ON START                         R7040031
 J     CAL K               ADVANCE ADDRESSES IN                         R7040032
       ADD TWFV            PRINT LOOP BY 25                             R7040033
       STA K               X                                            R7040034
       STA M               X                                            R7040035
       PXD 0,0             CLEAR AC                                     R7040036
       LXA TWFR,2          SET IR2=24                                   R7040037
       WPR                 SELECT PRINTER                               R7040038
       SPR 7                                                            R7040039
 K     CAD 0,2         PRINT COPY LOOP                                  R7040040
       TIX K,2,1           REPEAT UNTIL LINE COPIED                     R7040041
       SLW TEMP            TEST LOGICAL SUM                             77040042
       CLA TEMP            AGAINST STORED SUM                           R7040043
 M     SUB 0               X                                            R7040044
       TZE N               IF O, PROCEED                                R7040045
       HPR 7               IF NOT O, ERROR, STOP SHOWING 7              R7040046
 N     TXI G,1,32767       DECREASE IR1 BY 1                            R7040047
 TWFR  HTR 24              CONSTANT                                     R7040048
 TWFV  HTR 25              CONSTANT                                     R7040049
 P     HTR 100             INITIAL ADDRESS                              R7040050
 TEMP  HTR 0               TEMPORARY STORE                              R7040051
       END 24                                                           R7040052
