       REM MU BPU 2   0041 CARDS   BINARY PUNCH                         MUBPU201
 BPU2  SXD COMMON,1    STORE IR1                                        BPU20002
       SXD COMMON+1,2  STORE IR2                                        BPU20003
       CLA 2,4         PARAMETER N TO AC                                BPU20004
       PAX 0,1         SET IR1=N                                        BPU20005
       CLA 1,4         PARAMETERS R,S TO AC                             BPU20006
       STO COMMON+2    X                                                BPU20007
       ARS 18          SHIFT R TO ADDRESS OF AC                         BPU20008
       ADD 2,4         R+N                                              BPU20009
       STA BPU2+12     PLANT ADDRESS FOR CKS LOOP                       BPU20010
       WPU             SELECT PUNCH                                     BPU20011
       LXA BPU2+11,2   SET IR2=0                                        BPU20012
       PXD 0,0         CLEAR AC                                         BPU20013
       ACL 0,1         FORM CHECK SUM                                   BPU20014
       TXI BPU2+14,2,1 ADD 1 TO IR2                                     BPU20015
       TXH BPU2+16,2,21 TEST FOR FULL BINARY CARD                       BPU20016
       TIX BPU2+12,1,1 REPEAT UNTIL DONE                                BPU20017
       SLW COMMON+4    PARTIAL CHECK SUM TO 9R                          BPU20018
       PXD 0,2         V = NO. OF WORDS ON CARD                         BPU20019
       STO COMMON+3    V TO 9L DECREMENT                                BPU20020
       ADD COMMON+2    R+V IN DECREMENT, S IN ADDRESS                   BPU20021
       STA COMMON+3    S TO 9L ADDRESS                                  BPU20022
       ADD BPU2+39     S+22                                             BPU20023
       STO COMMON+2    SAVE FOR USE NEXT TIME                           BPU20024
       ARS 18          R+V TO ADDRESS OF AC                             BPU20025
       STA BPU2+32     PLANT ADDRESS FOR COPY LOOP                      BPU20026
       CLA BPU2+38     MURA IDENTIFICATION                              BPU20027
       ORS COMMON+3    COMPLETE 9L                                      BPU20028
       CAL COMMON+3    ADD 9L TO                                        BPU20029
       ACL COMMON+4    PARTIAL CHECK SUM                                BPU20030
       SLW COMMON+4    TOTAL CHECK SUM TO 9R                            BPU20031
       CPY COMMON+3    COPY 9L                                          BPU20032
       CPY COMMON+4    COPY 9R                                          BPU20033
       CPY 0,2         START OF COPY LOOP                               BPU20034
       TIX BPU2+32,2,1 LOOP RE-ENTRY, OUT AFTER V PASSES                BPU20035
       TIX BPU2+9,1,1  REPEAT UNTIL DONE                                BPU20036
       LXD COMMON,1    RESTORE IR1                                      BPU20037
       LXD COMMON+1,2  RESTORE IR2                                      BPU20038
       TRA 3,4         OUT                                              BPU20039
       HTR 0           CONSTANTS                                        BPU20040
       HTR 22          X                                                BPU20041
