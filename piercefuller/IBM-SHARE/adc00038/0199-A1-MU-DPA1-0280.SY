       REM MU DPA 1  0033 CARDS  DOUBLE PRECISION FLOATING POINT ADD    MUDPA101
 DPA1  STQ COMMON+2    STORE A(2)                                       DPA10002
       FAD COMMON      A(1) + B(1)                                      DPA10003
       STO COMMON+3    STORE MSP                                        DPA10004
       PXD 0,0         CLEAR AC                                         DPA10005
       LLS 35          LSP TO AC                                        DPA10006
       FAD COMMON+2    +A(2)                                            DPA10007
       FAD COMMON+1    +B(2)                                            DPA10008
       FAD COMMON+3    + MSP OF A(1) + B(1)                             DPA10009
       STO COMMON      STORE MSP OF SUM                                 DPA10010
       TQP DPA1+24                                                      DPA10011
       TMI DPA1+23     HERE IF MQ -, OUT IF AC -                        DPA10012
       SUB DPA1+30     HERE IF MQ -, AC +, - 1 IN 35TH BIT              DPA10013
       STO COMMON+3                                                     DPA10014
       CLM             CLEAR AC                                         DPA10015
       RQL 9           SEPARATE CHARACTERISTIC                          DPA10016
       LGL 27          FROM FRACTION OF LSP                             DPA10017
       TZE DPA1+27     IF ZERO, TRANSFER                                DPA10018
       SSM             - FRACTION OF LSP                                DPA10019
       ADD DPA1+31     1-FRACTION                                       DPA10020
       STQ COMMON+1                                                     DPA10021
       ORS COMMON+1    COMBINE CHARACTERISTIC AND FRACTION              DPA10022
       CLS COMMON+1    -LSP                                             DPA10023
       FAD COMMON+3    + MSP                                            DPA10024
       TRA 1,4         OUT                                              DPA10025
       TPL DPA1+23     HERE IF MQ +, OUT IF AC +                        DPA10026
       ADD DPA1+30     HERE IF MQ +, AC-, +1 IN 35TH BIT                DPA10027
       TRA DPA1+12                                                      DPA10028
       CLA COMMON      HERE IF LSP=0, REPLACE MSP                       DPA10029
       LRS             SIGN OF MSP REPLACES SIGN OF LSP                 DPA10030
       TRA 1,4         OUT                                              DPA10031
       OCT 000000000001 CONSTANTS                                       DPA10032
       OCT 001000000000                                                 DPA10033
