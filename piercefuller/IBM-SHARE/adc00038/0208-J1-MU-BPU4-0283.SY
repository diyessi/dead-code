       REM BPU4 MURA BINARY PUNCH 24 WORDS/CARD                           MUBPU4
 BPU4  LXA 0,4             SET X4 TO 0                                  BPU40001
       CPY 2,4             SELF-LOADING COPY LOOP                       BPU40002
       TXI BPU4+1,4,-1     RETURN TO COPY                               BPU40003
       HTR 1                                                            BPU40004
       HPR 8191            STOP FOR MQ ENTRIES                          BPU40005
       STQ BPU4+19         STORE ENTRIES                                BPU40006
       LLS 35              FIRST ADDRESS TO AC                          BPU40007
       COM                 2* COMPLEMENT TO X2                          BPU40008
       PAX 0,2             X                                            BPU40009
       TXI BPU4+10,2,1     X                                            BPU40010
       LXD BPU4+19,1       N=NO. OF WORDS TO X1                         BPU40011
       WPU                                                              BPU40012
       LXA BPU4+4,4                                                     BPU40013
       TXL BPU4+4,1,0      DOES N=0                                     BPU40014
       CPY 0,2             PUNCH WORD                                   BPU40015
       TXI BPU4+16,2,-1    INCREASE ADDRESS                             BPU40016
       TXI BPU4+17,1,-1    DECREASE N                                   BPU40017
       TIX BPU4+13,4,342   RETURN FOR NEXT WORD                         BPU40018
       TRA BPU4+11         READY NEXT CARD                              BPU40019
