       REM MU RDI 1  0084 CARDS   READ DECIMAL INTEGER ROUTINE          MURDI101
 RDI1  SXD COMMON+6,1  SAVEIR1                                          RDI10002
       SXD COMMON+7,2  SAVEIR2                                          RDI10003
       RCD             READ CARD READER                                 RDI10004
       CLA RDI1+80     SET T=10                                         RDI10005
       STO COMMON      X                                                RDI10006
       PXD 2,0         CLEAR CHARACTER (=CH) WORDS                      RDI10007
       STO COMMON+1    X                                                RDI10008
       STO COMMON+2    X                                                RDI10009
       CPY COMMON+3    COPY LEFT WORD                                   RDI10010
       TRA RDI1+61     TO RIGHT COPY                                    RDI10011
       TRA RDI1+29     END OF FILE                                      RDI10012
       CLA RDI1+19     END OF RECORD, CONVERT INTEGER                   RDI10013
       STA RDI1+24     SET SWITCH TO ADDRESS                            RDI10014
       LXA RDI1+5,2    SE IR2=2                                         RDI10015
       PXD 9,0         CLEAR AC                                         RDI10016
       STO COMMON+3    CLEAR SIGN WORD                                  RDI10017
       STO COMMON+5    CLEAR CONSTRUCTION WORD                          RDI10018
       LXA RDI1+14,1   SET IR1=9                                        RDI10019
       LDQ COMMON+3,2  CHARACTER WORD IN MQ                             RDI10020
       PXD RDI1+52,0   CLEAR AC                                         RDI10021
       LGL 4           CH IN AC                                         RDI10022
       STO COMMON      STORE CH                                         RDI10023
       TNZ RDI1+32     IF NOT 0, NOT A BLANK                            RDI10024
       CAL COMMON+5    IF 0 END CONSTRUCTION CNSTRCTD WORD IN AC        RDI10025
       TRA             SWITCH                                           RDI10026
       ACL COMMON+3    ADD SIGN                                         RDI10027
       SLW             STORE THE INTEGER                                RDI10028
       CAL COMMON+4    12R IN AC                                        RDI10029
       TZE RDI1+2      IF 0, READ NEXT CARD                             RDI10030
       LXD COMMON+6,1  IF NOT 0, RESTORE IR1                            RDI10031
       LXD COMMON+7,2  RESTORE IR2                                      RDI10032
       TRA 1,4         OUT TO PROGRAM                                   RDI10033
       SUB RDI1+81     CH-11 IN AC                                      RDI10034
       TNZ RDI1+37     IF NOT 0, CH NOT=11, DO NOT HAVE - SIGN          RDI10035
       SSM             OF 0, CH=11, HAVE - SIGN                         RDI10036
       STO COMMON+3    SET SIGN WORD -                                  RDI10037
       TRA RDI1+49     ADVANCE TO NEXT CH                               RDI10038
       TMI RDI1+41     IF -, HAVE CH LESS THAN 11                       RDI10039
       PXD RDI1+25,0   IF NOT -, HAVE CH=12, IEO, CLEAR AC              RDI10040
       STO COMMON      SET CH=0                                         RDI10041
       TRA RDI1+43     TO ARITHMETIC                                    RDI10042
       ADD RDI1+82     CH-10 IN AC                                      RDI10043
       TZE RDI1+49     IF 0, CH=10, IE A PLUS, TO NEXT CHARACTER        RDI10044
       CLA COMMON+5    IF NOT 0, CH LESS THAN 10                        RDI10045
       ALS 2           PERFORM NEXT STEP OF CONVERSION                  RDI10046
       ADD COMMON+5    X                                                RDI10047
       ALS 1           X                                                RDI10048
       ADD COMMON      X                                                RDI10049
       STO COMMON+5    X                                                RDI10050
       TIX RDI1+19,1,1 MOVE IN NEXT CHARACTER                           RDI10051
       TIX RDI1+17,2,1 MOVE TO NEXT CHARACTER WORD                      RDI10052
       TRA RDI1+23     HAVE READ 18, FORM AND STORE INTEGER             RDI10053
       TNZ RDI1+55     TEST ADDRESS FOR ZERO, IF NOT 0 USE AS IS        RDI10054
       CAL RDI1+26     IF 0, FORM OLD ADDRESS PLUS 1                    RDI10055
       ADD RDI1+82     X                                                RDI10056
       STA RDI1+26     RESTORE LOAD ADDRESS                             RDI10057
       CLA RDI1+38     SET SWITCH TO INTEGER                            RDI10058
       STA RDI1+24     X                                                RDI10059
       PXD 0,0         CLEAR AC                                         RDI10060
       STO COMMON+5    CLEAR CONSTRUCTION WORD                          RDI10061
       TRA RDI1+49     ADVANCE TO NEXT CHARACTER                        RDI10062
       CPY COMMON+4    COPY RIGHT WORD                                  RDI10063
       CLA COMMON      T IN AC                                          RDI10064
       SUB RDI1+82     T-1 IN AC                                        RDI10065
       TNZ RDI1+66     IF NOT 0, RESTORE                                RDI10066
       CLA RDI1+70     IF 0, SET T=12                                   RDI10067
       STA COMMON      RESTORE T                                        RDI10068
       LDQ COMMON+3    LEFT COPY WORD IN MQ                             RDI10069
       LXA RDI1+5,2    SET IR2=2                                        RDI10070
       LXA RDI1+14,1   SET IR1=9                                        RDI10071
       PXD 12,0        CLEAR AC                                         RDI10072
       ALS 4           SHIFT AC LEFT 4                                  RDI10073
       TQP RDI1+74     IF MQS=0, DO NOT ADD T                           RDI10074
       ADD COMMON      IF MQS=1, ADD T                                  RDI10075
       RQL 1           ROTATE Q LEFT 1                                  RDI10076
       TIX RDI1+71,1,1 GENERATE PARTIAL CHARACTER WORD                  RDI10077
       ACL COMMON+3,2  ADD CHARACTER WORD SO FAR                        RDI10078
       SLW COMMON+3,2  RESTORE CHARACTER WORD                           RDI10079
       TIX RDI1+69,2,1 REPEAT FOR NEXT CHARACTER WORD                   RDI10080
       TRA RDI1+8      COPY NEXT ROW                                    RDI10081
       HTR 10          CONSTANTS                                        RDI10082
       HTR 11          X                                                RDI10083
       HTR 1           X                                                RDI10084
