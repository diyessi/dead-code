   NY PCP20PERIPHERAL CARD PUNCHER                                   145NUDEK25M
       REM NYPCP2 0123 CARDS PERIPHERAL CARD READER                     PCP20001
 PCP2  SXD 1BX.K,1                                                      PCP20002
       SXD 2BX.K,2                                                      PCP20003
       SXD 4BX.K,4                                                      PCP20004
       REM SET REFERENCES FROM CALLING SEQUENCE                         PCP20005
       CAL 1,4                                                          PCP20006
       STA C.K T1                                                       PCP20007
       LRS 18                                                           PCP20008
       STA B.K                                                          PCP20009
       CAL 2,4                                                          PCP20010
       ACL NORM                                                         PCP20011
       STA K.K                                                          PCP20012
       COM                                                              PCP20013
       PDX 0,1                                                          PCP20014
       TXI A.K,1,1                                                      PCP20015
A.K    SXD ORG.K,1                                                      PCP20016
       REM SET RECORD TO BLANK                                          PCP20017
       CAL BNK.K                                                        PCP20018
       TXI A1.K,1,-13                                                   PCP20019
 A1.K  SLW 0,1                                                          PCP20020
       TXI ORG.K,1,1                                                    PCP20021
 ORG.K TXL A1.K,1,0                                                     PCP20022
       LXD ORG.K,1                                                      PCP20023
       REM SET CHARACTER COUNT AND LEVEL INDEX J                        PCP20024
       LXD MZR.K,6                                                      PCP20025
       TXI A2.K,2,6                                                     PCP20026
       REM SET UP FIRST WORD TO RECEIVE DIGIT                           PCP20027
 A2.K  LDQ 0,1                                                          PCP20028
       RQL 29                                                           PCP20029
       REM LOCATE WORD AND EXAMINE FORMAT-CONTROL TABLE TO FIX FIELDS   PCP20030
 B.K   CLA 0,4                                                          PCP20031
       STO INP.K                                                        PCP20032
 C.K   CLA 0,4                                                          PCP20033
       TZE E0R.K                                                        PCP20034
       SXD LVL.K,4                                                      PCP20035
       PAX 0,4                                                          PCP20036
       TMI SKP.K                                                        PCP20037
       REM GENERATE ONE DIGIT                                           PCP20038
 D.K   RQL 6                                                            PCP20039
       LLS 6                                                            PCP20040
       STQ WRD.K                                                        PCP20041
       CLA INP.K                                                        PCP20042
       LDQ MZR.K                                                        PCP20043
       LRS 2                                                            PCP20044
       ADD INP.K                                                        PCP20045
       LRS 32                                                           PCP20046
       STQ INP.K                                                        PCP20047
       REM JOIN CHARACTER GENERATED TO WORD                             PCP20048
       LDQ WRD.K                                                        PCP20049
       LLS 0                                                            PCP20050
       LRS 6                                                            PCP20051
       TNX F.K,4,1                                                      PCP20052
       TIX D.K,2,1                                                      PCP20053
       REM PROCESS COMPLETED WORD AND SET UP ANOTHER WORD               PCP20054
       RQL 7                                                            PCP20055
       STQ 0,1                                                          PCP20056
       LDQ 1,1                                                          PCP20057
       RQL 29                                                           PCP20058
       TXI E.K,2,5                                                      PCP20059
 E.K   TXI D.K,1,-1                                                     PCP20060
       REM AFFIX SIGN OVER LAST POSITION                                PCP20061
 F.K   CLA INP.K                                                        PCP20062
       TPL G1.K                                                         PCP20063
       CLM                                                              PCP20064
       LLS 6                                                            PCP20065
       TNZ G.K                                                          PCP20066
       ORA TEN.K                                                        PCP20067
 G.K   ORA MNS.K                                                        PCP20068
       LRS 6                                                            PCP20069
 G1.K  TIX I.K,2,1                                                      PCP20070
       REM PROCESS COMPLETED WORD AND SET UP ANOTHER WORD               PCP20071
       RQL 7                                                            PCP20072
       STQ 0,1                                                          PCP20073
       LDQ 1,1                                                          PCP20074
       RQL 29                                                           PCP20075
       TXI H.K,2,5                                                      PCP20076
 H.K   TXI I.K,1,-1                                                     PCP20077
       REM RETURN FOR NEXT FIELD                                        PCP20078
 I.K   LXD LVL.K,4                                                      PCP20079
       TXI B.K,4,-1                                                     PCP20080
       REM SKIP THIS FIELD                                              PCP20081
 SKP.K RQL 6                                                            PCP20082
       TIX P.K,2,1                                                      PCP20083
       RQL 7                                                            PCP20084
       STQ 0,1                                                          PCP20085
       LDQ 1,1                                                          PCP20086
       RQL 29                                                           PCP20087
       TXI O.K,2,5                                                      PCP20088
 O.K   TXI P.K,1,-1                                                     PCP20089
 P.K   TIX SKP.K,4,1                                                    PCP20090
       LXD LVL.K,4                                                      PCP20091
       TXI B.K,4,-1                                                     PCP20092
       REM TERMINATE PROCESSING                                         PCP20093
 E0R.K TXL J.K,2,5                                                      PCP20094
       TXI K.K,1,1                                                      PCP20095
 J.K   RQL 6                                                            PCP20096
       TIX J.K,2,1                                                      PCP20097
       RQL 7                                                            PCP20098
       STQ 0,1                                                          PCP20099
       REM COPY RECORD OUT TO SELECTED TAPE                             PCP20100
 K.K   WRS 0                                                            PCP20101
       SXD M.K,1                                                        PCP20102
       LXD ORG.K,1                                                      PCP20103
 L.K   CPY 0,1                                                          PCP20104
 M.K   TXL END.K,1,0                                                    PCP20105
       TXI L.K,1,-1                                                     PCP20106
       REM END OF PROGRAM-RETURN TO LINKAGE                             PCP20107
 END.K IOD                                                              PCP20108
       LXD MZR.K,7                                                      PCP20109
 1BX.K TXI 2BX.K,1,0                                                    PCP20110
 2BX.K TXI 4BX.K,2,0                                                    PCP20111
 4BX.K TXI OUT.K,4,0                                                    PCP20112
 OUT.K TRA 3,4                                                          PCP20113
       REM PROGRAM CONSTANTS                                            PCP20114
NORM       128                                                          PCP20115
 BNK.K OCT 606060606060                                                 PCP20116
 MZR.K MZE                                                              PCP20117
 TEN.K OCT 12                                                           PCP20118
 MNS.K OCT 40                                                           PCP20119
       REM ERASABLES                                                    PCP20120
 INP.K                                                                  PCP20121
 LVL.K                                                                  PCP20122
 WRD.K                                                                  PCP20123
