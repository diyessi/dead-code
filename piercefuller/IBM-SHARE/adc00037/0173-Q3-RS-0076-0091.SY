 CHECK PAX 0,1                                                          00760062
       LXD FOUR,4                                                       00760063
 NDEX2 LXD FOUR,2                                                       00760064
       WPU                                                              00760065
       CPY IDENT                                                        00760066
       CPY TEMP                                                         00760067
 RECNO CPY 0,4                                                          00760068
       TXI INCR4,4,1                                                    00760069
 INCR4 TXI INCR2,2,1                                                    00760070
 INCR2 TNX END,1,1                                                      00760071
       TXL RECNO,2,21                                                   00760072
       TRA NDEX2                                                        00760073
 LAST  SXD 3072,2                                                       00760074
 1DECR PZE 0,0,1                                                        00760075
 ONE   DEC 1                                                            00760076
