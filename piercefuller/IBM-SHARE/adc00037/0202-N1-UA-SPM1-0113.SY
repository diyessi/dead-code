       REM UA SPM 1 RD - READ DRUM PROGRAM (TRAP PRINT)                 UASPM1RW
       ORG                                                              SPM1RW01
           T,1                                                          SPM1RW02
                                                                        SPM1RW03
           T                                                            SPM1RW04
       RDR D                                                            SPM1RW05
           ,1                                                           SPM1RW06
       PXD FDL                                                          SPM1RW07
           ,1                                                           SPM1RW08
       LDA T+1                                                          SPM1RW09
           ,1                                                           SPM1RW10
       LXD T+10,4                                                       SPM1RW11
           ,1                                                           SPM1RW12
       CAD F+N,4                                                        SPM1RW13
           ,1                                                           SPM1RW14
       TIX T+4,4,1                                                      SPM1RW15
           ,1                                                           SPM1RW16
       CPY                                                              SPM1RW17
           ,1                                                           SPM1RW18
       SLW 1                                                            SPM1RW19
           ,1                                                           SPM1RW20
       CLA                                                              SPM1RW21
           ,1                                                           SPM1RW22
       SUB 1                                                            SPM1RW23
           ,1                                                           SPM1RW24
       TXI TEXIT,,N                                                     SPM1RW25
       REM                                                              SPM1RW26
       REM UA SPM 1 PM - READ DRUM PROGRAM (POST MORTUM PRINT)          SPM1RW27
       ORG                                                              SPM1RW28
       CPY R              REMOTE SELF-LOAD BOOTSTRAP                    SPM1RW29
       TXI R,,N           X                                             SPM1RW30
       CPY R+1            X                                             SPM1RW31
       TXI ,,13           X                                             SPM1RW32
       LXD R+1,4          X                                             SPM1RW33
       CPY R+14,4         X                                             SPM1RW34
       TIX R,4,1          X                                             SPM1RW35
       RDR D              SELECT DRUM                                   SPM1RW36
       PXD FDL            CLEAR ACC FOR CHECKSUM                        SPM1RW37
       LXD 1,4            SET WORD COUNT                                SPM1RW38
       LDA R+3            SET FIRST DRUM-STORAGE LOCATION               SPM1RW39
       CAD F+N,4          COPY AND COMPUTE CHECKSUM                     SPM1RW40
       TIX R+6,4,1        X                                             SPM1RW41
       CPY R              COPY DRUM CHECKSUM                            SPM1RW42
       SLW R+1            VERIFY RECOMPUTED CHECKSUM                    SPM1RW43
       CLA R              X                                             SPM1RW44
       SUB R+1            X                                             SPM1RW45
       TZE REXIT          OK EXIT FROM RD PROGRAM                       SPM1RW46
       HTR R+2            ERROR                                         SPM1RW47
       REM                                                              SPM1RW48
       REM UA SPM 1 WD - WRITE DRUM PROGRAM                             SPM1RW49
       ORG W                                                            SPM1RW50
           W,1            C/T CARD TRANSFER ADDRESS                     SPM1RW51
                                                                        SPM1RW52
           W                                                            SPM1RW53
       WDR D              SELECT DRUM                                   SPM1RW54
           ,1                                                           SPM1RW55
       PXD FDL            CLEAR ACC FOR CHECKSUM                        SPM1RW56
           ,1                                                           SPM1RW57
       LXD W+9,4          SET WORD COUNT                                SPM1RW58
           ,1                                                           SPM1RW59
       LDA W+1            SET FIRST DRUM-STORAGE LOCATION               SPM1RW60
           ,1                                                           SPM1RW61
       CAD F+N-1,4        COPY AND CALCULATE CHECKSUM                   SPM1RW62
           ,1                                                           SPM1RW63
       TIX W+4,4,1        X                                             SPM1RW64
           ,1                                                           SPM1RW65
       CAD F+N,4          X                                             SPM1RW66
           ,1                                                           SPM1RW67
       SLW W              STORE CHECKSUM                                SPM1RW68
           ,1                                                           SPM1RW69
       CPY W                ON DRUM                                     SPM1RW70
           ,1                                                           SPM1RW71
       TXI WEXIT,,N-1     EXIT FROM WD PROGRAM                          SPM1RW72
       REM                                                              SPM1RW73
       REM READ-WRITE PARAMETERS                                        SPM1RW74
 F     EQU 72             FIRST CORE STORAGE LOCATION                   SPM1RW75
 N     EQU 511-72+1       NUMBER OF CORE STORAGE LOCATIONS              SPM1RW76
 D     EQU 8              DRUM IDENTIFICATION                           SPM1RW77
 FDL   EQU 512            FIRST DRUM STORAGE LOCATION                   SPM1RW78
 WEXIT EQU 16             EXIT ADDRESS WD PROGRAM                       SPM1RW79
 REXIT EQU 72+7           EXIT ADDRESS RD PROGRAM                       SPM1RW80
 W     EQU                FIRST LOCATION OF WD PROGRAM                  SPM1RW81
 R     EQU 2              FIRST LOCATION OF RD PROGRAM                  SPM1RW82
 TEXIT EQU 72+5                                                         SPM1RW83
 T     EQU 2                                                            SPM1RW84
       END                                                              SPM1RW85
