   NY DCP20DIRECT CARD PUNCHER                                       145NUDEK24Q
       REM NYDCP2 0085 CARDS FREE-FIELD DIRECT CARD PUNCHER             DCP20001
 DCP2  SXD 1BX.C,1                                                      DCP20002
       SXD 2BX.C,2                                                      DCP20003
       SXD 4BX.C,4                                                      DCP20004
       REM SET REFERENCES FROM CALLING SEQUENCE                         DCP20005
       CAL 1,4                                                          DCP20006
       STA C.C                                                          DCP20007
       ARS 18                                                           DCP20008
       STA D.C                                                          DCP20009
       CAL MZR.C                                                        DCP20010
       SUB 2,4                                                          DCP20011
       PAX 0,2                                                          DCP20012
       SXD M.C,2                                                        DCP20013
       REM SET COLUMN INDICATOR AND J                                   DCP20014
       CAL MZR.C                                                        DCP20015
       SLW IND.C                                                        DCP20016
       LXD MZR.C,1                                                      DCP20017
       REM CLEAR CARD IMAGE                                             DCP20018
       SXD B.C,2                                                        DCP20019
       TXI A.C,2,-11                                                    DCP20020
 A.C   STO 0,2                                                          DCP20021
       STO 12,2                                                         DCP20022
       TXI B.C,2,1                                                      DCP20023
 B.C   TXL A.C,2,0                                                      DCP20024
       REM EXAMINE FORMAT-CONTROL TABLE TO FIX FIELDS                   DCP20025
       TXI C.C,2,-3                                                     DCP20026
 C.C   CLA 0,1                                                          DCP20027
       TZE EOR.C END OF RECORD                                          DCP20028
       PAX 0,4                                                          DCP20029
       TMI SKP.C                                                        DCP20030
       REM GENERATE ONE DIGIT                                           DCP20031
 D.C   CLA 0,1                                                          DCP20032
       STO INP.C                                                        DCP20033
 D1.C  CLA INP.C                                                        DCP20034
       LDQ MZR.C                                                        DCP20035
       LRS 2                                                            DCP20036
       ADD INP.C                                                        DCP20037
       LRS 32                                                           DCP20038
       STQ INP.C                                                        DCP20039
       STA E.C                                                          DCP20040
       CAL IND.C                                                        DCP20041
 E.C   ORS 0,2                                                          DCP20042
       REM AFFIX SIGN IF LAST DIGIT                                     DCP20043
       TXH F.C,4,1                                                      DCP20044
       TQP F.C                                                          DCP20045
       ORS -1,2                                                         DCP20046
       REM SHIFT COLUMN INDICATOR                                       DCP20047
 F.C   ARS 1                                                            DCP20048
       TNZ G.C                                                          DCP20049
       CAL MZR.C NEW INDICATOR NEEDED                                   DCP20050
       TXI G.C,2,-12 MOVE TO RIGHT HALF OF CARD IMAGE                   DCP20051
 G.C   SLW IND.C                                                        DCP20052
       TIX D1.C,4,1 RETURN FOR NEXT DIGIT                               DCP20053
       TXI C.C,1,-1 RETURN FOR NEXT FIELD                               DCP20054
       REM SKIP THIS FIELD                                              DCP20055
 SKP.C LDQ IND.C                                                        DCP20056
       CLS MZR.C                                                        DCP20057
       TNX H.C,4,36                                                     DCP20058
       RQL 36,4                                                         DCP20059
       TRA I.C                                                          DCP20060
 H.C   LGL 36,4                                                         DCP20061
       TNZ J.C                                                          DCP20062
 I.C   STQ IND.C                                                        DCP20063
       TXI K.C,2,-12                                                    DCP20064
 J.C   SLW IND.C                                                        DCP20065
 K.C   TXI C.C,1,-1                                                     DCP20066
       REM CARD IS GENERATED-COPY TO PUNCH                              DCP20067
 EOR.C LXD M.C,2                                                        DCP20068
       TXI L.C,2,-11                                                    DCP20069
 L.C   CPY 0,2                                                          DCP20070
       CPY 12,2                                                         DCP20071
       TXI M.C,2,1                                                      DCP20072
 M.C   TXL L.C,2,0                                                      DCP20073
       REM RESTORE AND RETURN                                           DCP20074
       LXD 1BX.C,1                                                      DCP20075
       LXD 2BX.C,2                                                      DCP20076
       LXD 4BX.C,4                                                      DCP20077
       TRA 3,4                                                          DCP20078
       REM CONSTANT                                                     DCP20079
 MZR.C MZE                                                              DCP20080
       REM ERASABLES                                                    DCP20081
 1BX.C                                                                  DCP20082
 2BX.C                                                                  DCP20083
 4BX.C                                                                  DCP20084
 IND.C                                                                  DCP20085
 INP.C                                                                  DCP20086
