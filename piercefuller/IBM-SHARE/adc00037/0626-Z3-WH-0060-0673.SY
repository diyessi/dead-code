WH  60 REM 0068 04209                                                   WH600001
       REM FORTRAN TIME FUNCTION TO READ TIME CLOCK                     WH600002
       REM WESTINGHOUSE ELECTRIC CORP. - ANALYTICAL DEPT.               WH600003
       REM FRANK ENGEL, JR.   SEPT. 22,1958                             WH600004
       REM                                                              WH600005
       REM X= CLOCKF(A,B)     SETS A=MONTH, B= DAY , AND X= TIME IN .001WH600006
       REM                    HOUR, ALL FLOATING POINT NOS.             WH600007
       REM                                                              WH600008
       REM CONTROL CARD FOR FN LIB                                      WH600009
       FUL                                                              WH600010
       MZE 0,,4         FN2 PROGRAM CARD, 4 WORDS                       WH600011
       OCT 634352334327 CHECK SUM                                       WH600012
       PZE LAST,,0      NO. OF WORDS                                    WH600013
       PZE -5,,0        NO. OF WORDS OF COMMON                          WH600014
       BCD 1CLOCK       ENTRY SYMBOL                                    WH600015
       PZE CLOCK        ENTRY LOCATION                                  WH600016
       REL                                                              WH600017
       ORG -1                                                           WH600018
COMMON BSS 1                                                            WH600019
 CLOCK RPR                                                              WH600020
       SPR 10                                                           WH600021
       SXD T1,1                                                         WH600022
       SXD T2,2                                                         WH600023
       SXD T4,4                                                         WH600024
       LXA L,3                                                          WH600025
 L     CAL -1,4                                                         WH600026
       LRS 18                                                           WH600027
       SUB L2                                                           WH600028
       TNZ L4           CLA OP IF ZERO                                  WH600029
       TXL L5,1,-2      BY-PASS IF CLA ALREADY FOUND                    WH600030
       LLS 18                                                           WH600031
       STA MONTH                                                        WH600032
       TXI *+1,1,7      SET IR1 TO 6 FOR SHIFT CONTROL IN TIME SEPARATN WH600033
       TXL L3,2,-2      EXIT IF BOTH A AND B FOUND                      WH600034
 L5    TXI L,4,1                                                        WH600035
 L4    TXL L5,2,-2      BY-PASS IF LDQ FOUND BEFORE                     WH600036
       SUB L1                                                           WH600037
       TNZ L5           LDQ OP                                          WH600038
       LLS 18                                                           WH600039
       STA DAY                                                          WH600040
       LXA L19,2        SET IR2 FOR CPY LOOP AND LDQ FLAG               WH600041
       TXH L5,1,-2      EXIT IF A ALSO FOUND, OTHERWISE GO BACK         WH600042
 L3    STZ COMMON                                                       WH600043
       CPY COMMON                                                       WH600044
       TIX *-1,2,1                                                      WH600045
 L9    PXD ,-                                                           WH600046
 L19   LGL 19,1         EFFECTIVE SHIFTS 13,5,18 RESPECTIVELY           WH600047
       ORA CHAR         TO FLOAT FIXED NO.                              WH600048
       STQ COMMON-1     SAVE RESIDUE                                    WH600049
       FAD CHAR         NORMALIZE                                       WH600050
       TXL OUT,1,1      END WITH TIME IN AC                             WH600051
 LDQ   LDQ COMMON-1                                                     WH600052
 DAY   STO **           DAY                                             WH600053
       TIX L9,1,13                                                      WH600054
 MONTH STO **           MONTH                                           WH600055
       TXI L9,1,8                                                       WH600056
 OUT   LXD T1,1                                                         WH600057
       LXD T2,2                                                         WH600058
       LXD T4,4                                                         WH600059
       TRA 1,4                                                          WH600060
 CHAR  OCT 233000000000                                                 WH600061
 L1    OCT 06000        LDQ                                             WH600062
 L2    OCT 50000        CLA                                             WH600063
 LAST  BSS 0                                                            WH600064
 T1    SYN COMMON-4                                                     WH600065
 T2    SYN COMMON-2                                                     WH600066
 T4    SYN COMMON-3                                                     WH600067
       END                                                              WH600068
