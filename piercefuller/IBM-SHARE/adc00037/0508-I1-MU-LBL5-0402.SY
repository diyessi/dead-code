       REM CHECK SUM CORRECTING LOWER BINARY LOADER                       MULBL5
 LBL5  LXA 0,4             CLEAR IR4                                    LBL50001
       CPY 2,4             COPY UNTIL                                   LBL50002
       TXI 1,4,-1          FIRST CARD OF LOADER IN                      LBL50003
       HTR 31              MASK FOR WORD COUNT                          LBL50004
       LTM                 LEAVE TRAPPING MODE                          LBL50005
       LXA 15,4            SET IR4 TO 10                                LBL50006
       RCD                 SELECT CARD READER                           LBL50007
       CPY 34,4            COPY UNTIL                                   LBL50008
       TIX 7,4,1           SECOND CARD OF LOADER IN                     LBL50009
       RCD                 SELECT CARD READER                           LBL50010
       CPY 0               9L IN 0 AND MQ                               LBL50011
       LLS 17              WORD COUNT IN AC ADDRESS                     LBL50012
       ANA 3               EXTRACT WORD COUNT (=WC)                     LBL50013
       CPY 1               9R IN 1 (CHECK SUM)                          LBL50014
       TZE 0               IF WC=0, HAVE TRANSFER CARD, GO TO IT        LBL50015
       PAX 10,6            IF WC NOT 0, PUT IT IN IR2 AND IR4           LBL50016
       ADD 0               WC+LA (LA=LOAD ADDRESS)                      LBL50017
       STA 21              PLANT WC+LA AS ADDRESS                       LBL50018
       STA 31              OF COPIES                                    LBL50019
       STA 22              AND AS ACL ADDRESS                           LBL50020
       CAL 0               PREPARE FOR LOGICAL SUM                      LBL50021
       CPY 0,4             COPY WORD FROM BINARY CARD                   LBL50022
       ACL 0,4             AND ADD IT TO LOGICAL SUM                    LBL50023
       TIX 21,4,1          REPEATING UNTIL ENTIRE CARD READ             LBL50024
       SLW 2               FORM (CARD SUM-CHECK SUM)                    LBL50025
       CLA 2               X                                            LBL50026
       SUB 1               X                                            LBL50027
       TZE 9               IF 0, PROCEED TO NEXT CARD                   LBL50028
       WPU                 IF NOT 0, SELECT PUNCH                       LBL50029
       CPY 0               COPY 9L                                      LBL50030
       CPY 2               COPY CORRECT CHECK SUM                       LBL50031
       CPY 0,2             COPY REMAINDER OF CARD                       LBL50032
       TIX 31,2,1          REPEATING UNTIL DONE                         LBL50033
       TRA 9               READ NEXT BINARY CARD                        LBL50034
