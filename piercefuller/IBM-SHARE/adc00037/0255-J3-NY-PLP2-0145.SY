       REM NYPLP2 0126 CARDS PERIPHERAL LINE PRINTER                    PLP20001
 PLP2  SXD 1BX.N,1                                                      PLP20002
       SXD 2BX.N,2                                                      PLP20003
       SXD 4BX.N,4                                                      PLP20004
       REM SET REFERENCES FROM CALLING SEQUENCE                         PLP20005
       CAL 1,4                                                          PLP20006
       STA C.N T1                                                       PLP20007
       LRS 18                                                           PLP20008
       STA B.N T2                                                       PLP20009
       CAL 2,4                                                          PLP20010
       ACL NRM.N                                                        PLP20011
       STA K.N TAPE                                                     PLP20012
       COM                                                              PLP20013
       PDX 0,1                                                          PLP20014
       TXI A.N,1,1                                                      PLP20015
 A.N   SXD ORG.N,1                                                      PLP20016
       REM SET RECORD TO BLANK                                          PLP20017
       CAL BNK.N                                                        PLP20018
       TXI A1.N,1,-19                                                   PLP20019
 A1.N  SLW 0,1                                                          PLP20020
       TXI ORG.N,1,1                                                    PLP20021
 ORG.N TXL A1.N,1,0                                                     PLP20022
       LXD ORG.N,1                                                      PLP20023
       REM SET CHARACTER COUNT                                          PLP20024
       LXD MZR.N,6                                                      PLP20025
       TXI A2.N,2,6                                                     PLP20026
       REM SET UP FIRST WORD TO RECEIVE DIGIT                           PLP20027
 A2.N  LDQ 0,1                                                          PLP20028
       RQL 29                                                           PLP20029
       REM LOCATE WORD AND EXAMINE FORMAT-CONTROL TABLE TO FIX FIELDS   PLP20030
 B.N   CLA 0,4                                                          PLP20031
       STO INP.N                                                        PLP20032
 C.N   CLA 0,4                                                          PLP20033
       TZE E0R.N                                                        PLP20034
       SXD LVL.N,4                                                      PLP20035
       PAX 0,4                                                          PLP20036
       TMI SKP.N                                                        PLP20037
       REM GENERATE ONE DIGIT                                           PLP20038
 D.N   RQL 6                                                            PLP20039
       LLS 6                                                            PLP20040
       STQ WRD.N                                                        PLP20041
       CLA INP.N                                                        PLP20042
       LDQ MZR.N                                                        PLP20043
       LRS 2                                                            PLP20044
       ADD INP.N                                                        PLP20045
       LRS 32                                                           PLP20046
       STQ INP.N                                                        PLP20047
       REM JOIN CHARACTER GENERATED TO WORD                             PLP20048
       LDQ WRD.N                                                        PLP20049
       LLS 0                                                            PLP20050
       LRS 6                                                            PLP20051
       TNX F.N,4,1                                                      PLP20052
       TIX D.N,2,1                                                      PLP20053
       REM PROCESS COMPLETED WORD AND SET UP ANOTHER WORD               PLP20054
       RQL 7                                                            PLP20055
       STQ 0,1                                                          PLP20056
       LDQ 1,1                                                          PLP20057
       RQL 29                                                           PLP20058
       TXI E.N,2,5 RESTORE CHACTER COUNT                                PLP20059
 E.N   TXI D.N,1,-1                                                     PLP20060
       REM ATTACH SIGN AFT OF FIELD                                     PLP20061
 F.N   TIX F1.N,2,1                                                     PLP20062
       RQL 7                                                            PLP20063
       STQ 0,1                                                          PLP20064
       LDQ 1,1                                                          PLP20065
       RQL 29                                                           PLP20066
       TXI F2.N,2,5                                                     PLP20067
 F2.N  TXI F3.N,1,-1                                                    PLP20068
 F1.N  RQL 6                                                            PLP20069
 F3.N  CLA INP.N                                                        PLP20070
       TPL G1.N                                                         PLP20071
       LLS 6                                                            PLP20072
       CLM                                                              PLP20073
       ORA MNS.N                                                        PLP20074
       LRS 6                                                            PLP20075
 G1.N  TIX I.N,2,1                                                      PLP20076
       RQL 7                                                            PLP20077
       STQ 0,1                                                          PLP20078
       LDQ 1,1                                                          PLP20079
       RQL 29                                                           PLP20080
       TXI H.N,2,5                                                      PLP20081
 H.N   TXI I.N,1,-1                                                     PLP20082
       REM RETURN FOR NEXT FIELD                                        PLP20083
 I.N   LXD LVL.N,4                                                      PLP20084
       TXI B.N,4,-1                                                     PLP20085
       REM SKIP THIS FIELD                                              PLP20086
 SKP.N RQL 6                                                            PLP20087
       TIX P.N,2,1                                                      PLP20088
       RQL 7                                                            PLP20089
       STQ 0,1                                                          PLP20090
       LDQ 1,1                                                          PLP20091
       RQL 29                                                           PLP20092
       TXI O.N,2,5                                                      PLP20093
 O.N   TXI P.N,1,-1                                                     PLP20094
 P.N   TIX SKP.N,4,1                                                    PLP20095
       TRA I.N                                                          PLP20096
       REM TERMINATE PROCESSING                                         PLP20097
 E0R.N TXL J.N,2,5                                                      PLP20098
       TXI K.N,1,1                                                      PLP20099
 J.N   RQL 6                                                            PLP20100
       TIX J.N,2,1                                                      PLP20101
       RQL 7                                                            PLP20102
       STQ 0,1                                                          PLP20103
       REM COPY RECORD OUT TO SELECTED TAPE                             PLP20104
 K.N   WRS 0                                                            PLP20105
       SXD M.N,1                                                        PLP20106
       LXD ORG.N,1                                                      PLP20107
 L.N   CPY 0,1                                                          PLP20108
 M.N   TXL END.N,1,0                                                    PLP20109
       TXI L.N,1,-1                                                     PLP20110
       REM END OF PROGRAM-RETURN TO LINKAGE                             PLP20111
 END.N IOD                                                              PLP20112
       LXD MZR.N,7                                                      PLP20113
 1BX.N TXI 2BX.N,1,0                                                    PLP20114
 2BX.N TXI 4BX.N,2,0                                                    PLP20115
 4BX.N TXI OUT.N,4,0                                                    PLP20116
 OUT.N TRA 3,4                                                          PLP20117
       REM CONSTANTS                                                    PLP20118
 NRM.N     128                                                          PLP20119
 BNK.N OCT 606060606060                                                 PLP20120
 MZR.N MZE                                                              PLP20121
 MNS.N OCT 14                                                           PLP20122
       REM ERASABLES                                                    PLP20123
 INP.N                                                                  PLP20124
 LVL.N                                                                  PLP20125
 WRD.N                                                                  PLP20126
