       REM MU SBL 2  0026 CARDS   SHIFTING BINARY LOADER-ONE CARD       MUSBL201
 SBL2  LXA 0,4             CLEAR IR4                                    SBL20002
       CPY 2,4             COPYLOADER INTO 2,3, ETC.                    SBL20003
       TXI 1,4,32767       REPEAT UNTIL LOADER IN                       SBL20004
       HTR 0               SHIFTING INCREMENT ( HAND PUNCHED )          SBL20005
       RCD                 SELECT CARD READER                           SBL20006
       CPY 0               9L IN O AND MQ                               SBL20007
       LLS 17              WORD COUNT (=WC) IN AC ADDRESS               SBL20008
       ANA 23              EXTRACT WORD COUNT                           SBL20009
       CPY 1               9R IN 1 (CHECK SUM)                          SBL20010
       TZE 0               IF WC=O, HAVE TRANSFER CARD, GO TO IT        SBL20011
       PAX 0,4             SET IR4=WC                                   SBL20012
       ADD 0               (WC+LA) IN AC (LA=LOAD ADDRESS)              SBL20013
       ADD 3               (WC+LA+INC) IN AC                            SBL20014
       STA 16              PLANT (WC+LA+INC) AS CPY ADDRESS             SBL20015
       STA 17              AND AS ACL ADDRESS                           SBL20016
       CAL 0               PREPARE FOR CARD SUM                         SBL20017
       CPY 0,4             COPY WORD FROM BINARY CARD                   SBL20018
       ACL 0,4             AND ADD IT TO CARD SUM                       SBL20019
       TIX 16,4,1          REPEATING UNTIL DONE                         SBL20020
       SLW 2               FORM (CARD SUM)-(CHECK SUM)                  SBL20021
       CLA 2               X                                            SBL20022
       SUB 1               X                                            SBL20023
       TZE 4               IF O, PROCEED TO NEXT CARD                   SBL20024
       HTR 31              MASK FOR WORD COUNT (HALT FOR ERROR)         SBL20025
       END 0                                                            SBL20026
