   NY DCR20DIRECT CARD READER                                        145NUDEK24R
       REM NYDCR2 0223 CARDS DIRECT CARD READER                         DCR20001
 DCR2  SXD 1BX.D,1                                                      DCR20002
       SXD 2BX.D,2                                                      DCR20003
       SXD 4BX.D,4                                                      DCR20004
       REM INTERROGATE CALLING SEQUENCE FOR TABLE ORIGINS               DCR20005
       CAL 1,4 FOR T1                                                   DCR20006
       STA A.D                                                          DCR20007
       STA K1.D                                                         DCR20008
       STA X.D                                                          DCR20009
       STA EE.D                                                         DCR20010
       ARS 18 FOR T2                                                    DCR20011
       STA B.D                                                          DCR20012
       STA M.D                                                          DCR20013
       STA N.D                                                          DCR20014
       STA P.D                                                          DCR20015
       STA Q.D                                                          DCR20016
       STA Y1.D                                                         DCR20017
       STA GG.D                                                         DCR20018
       STA HH.D                                                         DCR20019
       CAL 2,4 FOR T4                                                   DCR20020
       STA OO.D                                                         DCR20021
       COM                                                              DCR20022
       PDX 0,2                                                          DCR20023
       TXI 9.D,2,1                                                      DCR20024
       REM CLEAR WORKSPACE AND BLANK COLUMN REGISTERS                   DCR20025
 9.D   LGL 36                                                           DCR20026
       STQ LBC.D                                                        DCR20027
       STQ RBC.D                                                        DCR20028
       REM EXAMINE FORMAT-CONTROL TABLE TO PLACE ZEROES                 DCR20029
       LXD MZR.D,1                                                      DCR20030
 A.D   CLA 0,1                                                          DCR20031
       TZE D.D                                                          DCR20032
       TMI C.D                                                          DCR20033
       REM SET ZERO AND RETURN                                          DCR20034
 B.D   STQ 0,1                                                          DCR20035
 C.D   TXI A.D,1,-1                                                     DCR20036
       REM SET TO COPY NINE ROWS FROM NINE-ROW TO ONE ROW               DCR20037
 D.D   TXI E.D,2,-3                                                     DCR20038
 E.D   SXD U.D,2                                                        DCR20039
       TXI G.D,2,-8                                                     DCR20040
       REM COPY-ACCUMULATE LEFT ROW AND EXIT ON EOF                     DCR20041
 G.D   CAL LBC.D                                                        DCR20042
       CPY 0,2                                                          DCR20043
       TRA H.D                                                          DCR20044
       TRA EOF.D                                                        DCR20045
 H.D   ORA 0,2                                                          DCR20046
       SLW CLR.D                                                        DCR20047
       SLW LBC.D                                                        DCR20048
       REM COPY-ACCUMULATE RIGHT ROW                                    DCR20049
 I.D   CAL RBC.D                                                        DCR20050
 J.D   CPY 12,2                                                         DCR20051
       ORA 12,2                                                         DCR20052
       SLW RBC.D                                                        DCR20053
       SLW CRR.D                                                        DCR20054
       SXD ROW.D,2                                                      DCR20055
       REM SET FIELDS FROM FORMAT-CONTROL TABLE                         DCR20056
 K.D   LXA MZR.D,1                                                      DCR20057
 K1.D  CLS 0,1                                                          DCR20058
       TZE T.D                                                          DCR20059
       STA S.D                                                          DCR20060
       TPL R.D                                                          DCR20061
       ADD G.D+1                                                        DCR20062
       PAX 0,2                                                          DCR20063
       REM READY ROW AND TEST FIELD LENGTH                              DCR20064
       LDQ CLR.D                                                        DCR20065
       TXH O.D,2,-6                                                     DCR20066
       REM LARGE FIELD FOLLOWED BY SMALL FIELD                          DCR20067
       TXI L.D,2,5                                                      DCR20068
 L.D   CLM                                                              DCR20069
       LGL 0,2                                                          DCR20070
 L1.D  PAX -5,4                                                         DCR20071
       CAL DCB.D+31,4                                                   DCR20072
 M.D   ACL 0,1                                                          DCR20073
 N.D   SLW 0,1                                                          DCR20074
       LXA L1.D,2                                                       DCR20075
       REM SMALL FIELD                                                  DCR20076
 O.D   CLM                                                              DCR20077
       LGL 0,2                                                          DCR20078
       PAX 0,4                                                          DCR20079
       CAL DCB.D+63,4                                                   DCR20080
 P.D   ACL 0,1                                                          DCR20081
 Q.D   SLW 0,1                                                          DCR20082
       REM SHIFT ENTIRE ROW LEFT TO SET UP NEXT FIELD                   DCR20083
 R.D   CAL CLR.D                                                        DCR20084
       LDQ CRR.D                                                        DCR20085
 S.D   LGL 0                                                            DCR20086
       SLW CLR.D                                                        DCR20087
       STQ CRR.D                                                        DCR20088
       REM RETURN TO FORMAT-CONTROL TABLE FOR NEXT FIELD                DCR20089
       TXI K1.D,1,-1                                                    DCR20090
       REM RETURN TO COPY NEXT ROW UNLESS 1-ROW IS PAST                 DCR20091
 T.D   LXD ROW.D,2                                                      DCR20092
       TXI U.D,2,1                                                      DCR20093
 U.D   TXL G.D,2,0                                                      DCR20094
       REM COPY-ACCUMULATE ZERO ROW LEFT                                DCR20095
       CAL LBC.D                                                        DCR20096
       CPY 0,2                                                          DCR20097
       ORA 0,2                                                          DCR20098
       SLW LBC.D                                                        DCR20099
       REM COPY ACCULULATE ZERO ROW RIGHT                               DCR20100
 V.D   CAL RBC.D                                                        DCR20101
       CPY 12,2                                                         DCR20102
       ORA 12,2                                                         DCR20103
       SLW RBC.D                                                        DCR20104
       REM SET ERROR SIGNAL                                             DCR20105
       CLM                                                              DCR20106
       SLW SIG.D                                                        DCR20107
       REM EXAMINE FORMAT-CONTROL TABLE TO LIMIT TESTS                  DCR20108
       LXA MZR.D,1                                                      DCR20109
 X.D   CLA 0,1                                                          DCR20110
       TZE BB.D                                                         DCR20111
       STA AA.D                                                         DCR20112
       TMI Z.D                                                          DCR20113
       STA Y.D                                                          DCR20114
       REM READY ROW TO TEST BLANK COLUMNS                              DCR20115
       CAL BCT.D                                                        DCR20116
       LDQ LBC.D                                                        DCR20117
       REM TEST                                                         DCR20118
 Y.D   LGL 0                                                            DCR20119
       COM                                                              DCR20120
       TZE Z.D                                                          DCR20121
       REM BLANK COLUMN CAUGHT                                          DCR20122
       SXD RBX.D,1                                                      DCR20123
       CLA MZR.D                                                        DCR20124
       STO SIG.D                                                        DCR20125
Y1.D   STO 0,1                                                          DCR20126
       REM SHIFT ENTIRE ROW LEFT TO SET UP NEXT FIELD                   DCR20127
 Z.D   CAL LBC.D                                                        DCR20128
       LDQ RBC.D                                                        DCR20129
 AA.D  LGL 0                                                            DCR20130
       SLW LBC.D                                                        DCR20131
       STQ RBC.D                                                        DCR20132
       REM RETURN TO FORMAT-CONTROL TABLE FOR NEXT FIELD                DCR20133
       TXI X.D,1,-1                                                     DCR20134
       REM COPY ELEVEN ROW LEFT                                         DCR20135
 BB.D  CPY -1,2                                                         DCR20136
       STQ LR.D                                                         DCR20137
       REM COPY ELEVEN-ROW RIGHT                                        DCR20138
       CPY 11,2                                                         DCR20139
       STQ RR.D                                                         DCR20140
       REM EXAMINE FORMAT-CONTROL TABLE FOR SIGN SETTING                DCR20141
       LXA MZR.D,1                                                      DCR20142
 EE.D  CLA 0,1                                                          DCR20143
       TZE KK.D                                                         DCR20144
       STA JJ.D                                                         DCR20145
       TMI II.D                                                         DCR20146
       STA FF.D                                                         DCR20147
       REM TEST PLURAL SIGNS                                            DCR20148
       CLM                                                              DCR20149
       LDQ LR.D                                                         DCR20150
 FF.D  LGL 0                                                            DCR20151
       PAX 0,4                                                          DCR20152
       TXL GG.D,4,1                                                     DCR20153
       REM PLURAL SIGNS CAUGHT                                          DCR20154
       SXD RBX.D,1                                                      DCR20155
       CLA MZR.D                                                        DCR20156
       STO SIG.D                                                        DCR20157
       TRA HH.D                                                         DCR20158
       REM SET SINGLE SIGNS                                             DCR20159
 GG.D  CLA 0,1                                                          DCR20160
       TRA HH.D,4                                                       DCR20161
       SSM                                                              DCR20162
 HH.D  STO 0,1                                                          DCR20163
       REM SHIFT ENTIRE ROW LEFT TO SET UP NEXT FIELD                   DCR20164
 II.D  CAL LR.D                                                         DCR20165
       LDQ RR.D                                                         DCR20166
 JJ.D  LGL 0                                                            DCR20167
       SLW LR.D                                                         DCR20168
       STQ RR.D                                                         DCR20169
       REM RETURN TO FORMAT-CONTROL TABLE FOR NEXT FIELD                DCR20170
       TXI EE.D,1,-1                                                    DCR20171
       REM COPY TWELVE ROWS                                             DCR20172
 KK.D  CPY -2,2                                                         DCR20173
 KK1.D CPY 10,2                                                         DCR20174
       REM ON END OF RECORD RESTORE INDEX REGISTERS                     DCR20175
 E0R.D LXD 1BX.D,1                                                      DCR20176
       LXD 2BX.D,2                                                      DCR20177
       LXD 4BX.D,4                                                      DCR20178
       REM TEST SIGNAL TO SELECT RETURN                                 DCR20179
       CLA SIG.D                                                        DCR20180
       TPL 5,4 ALL IS WELL.NORMAL RETURN                                DCR20181
       LXD RBX.D,1 ERROR HAS BEEN MADE                                  DCR20182
 OO.D  LDQ 0,1 T4+J TO MQ                                               DCR20183
       CAL SIG.D                                                        DCR20184
       SUB RBX.D                                                        DCR20185
       PDX 0,1                                                          DCR20186
       PXD 0,1                                                          DCR20187
       ARS 18                                                           DCR20188
       LXD 1BX.D,1                                                      DCR20189
       TRA 3,4 ERROR RETURN                                             DCR20190
       REM END OF FILE HAS BEEN MET AT READER                           DCR20191
 EOF.D LXD 1BX.D,1                                                      DCR20192
       LXD 2BX.D,2                                                      DCR20193
       LXD 4BX.D,4                                                      DCR20194
       TRA 4,4                                                          DCR20195
       REM PROGRAM CONSTANTS                                            DCR20196
 MZR.D MZE                                                              DCR20197
 BCT.D OCT 777777777777                                                 DCR20198
 DCB.D DEC 1111100000,1111000000,1110100000,1110000000                  DCR20199
       DEC 1101100000,1101000000,1100100000,1100000000                  DCR20200
       DEC 1011100000,1011000000,1010100000,1010000000                  DCR20201
       DEC 1001100000,1001000000,1000100000,1000000000                  DCR20202
       DEC 111100000,111000000,110100000,110000000                      DCR20203
       DEC 101100000,101000000,100100000,100000000                      DCR20204
       DEC 11100000,11000000,10100000,10000000                          DCR20205
       DEC 1100000,1000000,100000,0                                     DCR20206
       DEC 11111,11110,11101,11100,11011,11010,11001,11000              DCR20207
       DEC 10111,10110,10101,10100,10011,10010,10001,10000              DCR20208
       DEC 1111,1110,1101,1100,1011,1010,1001,1000                      DCR20209
       DEC 111,110,101,100,11,10,1,0                                    DCR20210
       REM ERASABLE STORAGE                                             DCR20211
 1BX.D                                                                  DCR20212
 2BX.D                                                                  DCR20213
 4BX.D                                                                  DCR20214
 LBC.D                                                                  DCR20215
 RBC.D                                                                  DCR20216
 ROW.D                                                                  DCR20217
 SIG.D                                                                  DCR20218
 CRR.D                                                                  DCR20219
 LR.D  SYN LBC.D                                                        DCR20220
 RR.D  SYN RBC.D                                                        DCR20221
 CLR.D SYN SIG.D                                                        DCR20222
